headers = stdatomic.h
---

static inline _Bool native_cas(_Atomic uint64_t* obj, uint64_t* expected, uint64_t desired ) {
    return atomic_compare_exchange_weak(obj, expected, desired);
}

static inline void native_atomic_write(_Atomic uint64_t* obj, uint64_t desired) {
    atomic_store(obj, desired);
}
